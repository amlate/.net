@using Webdiyer.WebControls.Mvc
@model Webdiyer.WebControls.Mvc.PagedList<AiJiaXi.Domain.Entities.JoinApplication>


@{
    ViewBag.Title = "代理商申请管理";
    ViewBag.Top = "代理商申请管理";
    ViewBag.Second = "代理商申请列表";
}

<div id="datatable">
    @Html.Partial("_JoinApplicationList", Model)
</div>
@section scripts
{
    @{Html.RegisterMvcPagerScriptResource();}
    <script>
        $(function() {
            $('.user-editable').editable({
                url: '@Url.Action("Post")',
                showbuttons: true
            });
        })
    </script>
}
